{
  "name": "grommet-dashboard",
  "version": "1.1.0",
  "type": "module",
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0.1"
  },
  "keywords": [
    "grommet",
    "grommet-dashboard",
    "admin-dashboard",
    "react",
    "react-dashboard",
    "nextjs",
    "nextjs-dashboard"
  ],
  "scripts": {
